    "vinyl-source-stream": "1.1.0",
    "xo": "0.16.0"
  },
  "homepage": "https://github.com/JamieMason/Jasmine-Matchers",
  "keywords": [
    "BDD",
    "TDD",
    "jasmine",
    "testing"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/JamieMason/Jasmine-Matchers.git"
  },
  "scripts": {
    "build": "gulp browserify",
    "development-watchers": "gulp development-watchers",
    "lint": "xo --fix",
    "saucelabs": "karma start karma-saucelabs.conf.js",
    "saucelabs-android": "npm run saucelabs -- --browsers android-5.1,android-5.0,android-4.4,android-4.3,android-4.2,android-4.1",
    "saucelabs-chrome": "npm run saucelabs -- --browsers chrome-52,chrome-51,chrome-50,chrome-49,chrome-48,chrome-47,chrome-46,chrome-45,chrome-44,chrome-43,chrome-42,chrome-41,chrome-40,chrome-39,chrome-38,chrome-37,chrome-36,chrome-35,chrome-34,chrome-33,chrome-32,chrome-31,chrome-30,chrome-29,chrome-28,chrome-27,chrome-26",
    "saucelabs-firefox": "npm run saucelabs -- --browsers firefox-48,firefox-47,firefox-46,firefox-45,firefox-44,firefox-43,firefox-42,firefox-41,firefox-40,firefox-39,firefox-38,firefox-37,firefox-36,firefox-35,firefox-34,firefox-33,firefox-32,firefox-31,firefox-30,firefox-29,firefox-28,firefox-27,firefox-26,firefox-25,firefox-24,firefox-23,firefox-22,firefox-21,firefox-20,firefox-19,firefox-18,firefox-17,firefox-16,firefox-15,firefox-14,firefox-13,firefox-12,firefox-11,firefox-10,firefox-9,firefox-8,firefox-7,firefox-6,firefox-5,firefox-4",
    "saucelabs-ie": "npm run saucelabs -- --browsers internet-explorer-11,internet-explorer-10,internet-explorer-9",
    "saucelabs-ios": "npm run saucelabs -- --browsers ipad-9.3,ipad-9.2,ipad-9.1,ipad-9.0,ipad-8.4,ipad-8.3,ipad-8.2,ipad-8.1",
    "saucelabs-opera": "npm run saucelabs -- --browsers opera-12,opera-11",
    "saucelabs-safari": "npm run saucelabs -- --browsers safari-9,safari-8,safari-7,safari-6",
    "saucelabs-unsupported": "npm run saucelabs -- --browsers android-4.0,internet-explorer-8,ipad-7.1,ipad-7.0,microsoftedge-13,safari-5",
    "test": "karma start",
    "test-watcher": "karma start --no-single-run --auto-watch"
  },
  "xo": {
    "envs": [
      "jasmine",
      "node"
    ],
    "esnext": false,
    "globals": [
      "any"
    ],
    "rules": {
      "max-nested-callbacks": 0,
      "no-array-constructor": 0,
      "no-new-object": 0,
      "no-new-wrappers": 0,
      "xo/filename-case": 0
    },
    "space": 2
  }
}
