#!/bin/bash

DASH_DOCROOT="$PWD/build/Jasmine-Matchers.docset/Contents/Resources"
# DOCS_DOCROOT="$PWD/docs"
TMPL="$PWD/node_modules/jsdoc-dash-template"

# Create Docs
jsdoc --recurse --destination "$DASH_DOCROOT/Documents" --template "$TMPL" src
# jsdoc --recurse --destination "$DOCS_DOCROOT" --template "$TMPL" src

# Clean Database
rm "$DASH_DOCROOT/docSet.dsidx"

